{
  "name": "composite-back",
  "version": "1.0.0-next.16",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:staging": "env-cmd -f .env.staging -- nest build",
    "build:dev": "env-cmd -f .env.dev -- nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "env-cmd -f .env -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate-swagger:local": "env-cmd -f .env.local ts-node -r tsconfig-paths/register ./src/scripts/generateSwaggerSpec.ts",
    "prisma:migrate:reset:local": "env-cmd -f .env.local prisma migrate reset",
    "prisma:migrate:deploy": "prisma migrate deploy"
  },
  "dependencies": {
    "@benjaminbours/composite-core": "^1.0.0-next.16",
    "@injectit/threejs-nodejs-exporters": "^0.0.2",
    "@nestjs/cache-manager": "^2.2.1",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/platform-socket.io": "^10.3.3",
    "@nestjs/serve-static": "^4.0.1",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/websockets": "^10.3.3",
    "@prisma/client": "^5.9.1",
    "@socket.io/redis-adapter": "^8.2.1",
    "@socket.io/redis-streams-adapter": "^0.1.0",
    "argon2": "^0.31.2",
    "cache-manager": "^5.4.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "env-cmd": "^10.1.0",
    "i18next": "^23.8.3",
    "i18next-fs-backend": "^2.3.1",
    "mjml": "^4.15.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.11",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport-jwt": "^4.0.1",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "short-unique-id": "^5.0.3",
    "socket.io": "^4.7.2",
    "three": "^0.161.0",
    "three-mesh-bvh": "^0.7.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.19",
    "@types/nodemailer": "^6.4.14",
    "@types/nodemailer-smtp-transport": "^2.7.8",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.14",
    "@types/three": "^0.161.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^5.9.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json"
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/setup-tests.ts"
    ],
    "moduleNameMapper": {
      "^@project-common/(.*)$": "<rootDir>/../src/common/$1"
    }
  },
  "prisma": {
    "seed": "npx ts-node -r tsconfig-paths/register prisma/seed.ts"
  }
}
